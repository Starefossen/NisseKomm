{
  "name": "my-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "npm run typecheck && npm run lint && npm run knip && npm run format",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --silent",
    "test:debug": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:badges": "jest --silent __tests__/lib/badge/",
    "format:check": "prettier --check .",
    "format": "prettier --write --list-different .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "16.0.1",
    "pixelarticons": "^1.8.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-sounds": "^1.0.25"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^30.2.0",
    "knip": "^5.68.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
