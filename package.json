{
  "name": "nissekomm",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run knip && pnpm run format && pnpm test",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check .",
    "format": "prettier --write --list-different .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "html5-qrcode": "^2.3.8",
    "next": "16.0.3",
    "pixelarticons": "^1.8.1",
    "qr-code-styling": "^1.9.2",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.70.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  }
}
