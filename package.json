{
  "name": "nissekomm",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run knip && pnpm run format && pnpm test",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check .",
    "format": "prettier --write --list-different .",
    "typecheck": "tsc --noEmit",
    "sanity": "sanity",
    "sanity:dev": "SANITY_STUDIO_DATASET=development sanity",
    "sanity:prod": "SANITY_STUDIO_DATASET=production sanity",
    "cleanup:test-sessions": "tsx scripts/cleanup-test-sessions.ts",
    "migrate:dev": "tsx scripts/migrate-sessions.ts --dataset development",
    "migrate:prod": "tsx scripts/migrate-sessions.ts --dataset production",
    "migrate:dry-run": "tsx scripts/migrate-sessions.ts --dry-run"
  },
  "dependencies": {
    "@sanity/client": "^7.13.1",
    "@sanity/vision": "^3.99.0",
    "@vercel/analytics": "^1.5.0",
    "html5-qrcode": "^2.3.8",
    "next": "^16.0.6",
    "pixelarticons": "^1.8.1",
    "qr-code-styling": "^1.9.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sanity": "^4.19.0",
    "styled-components": "^6.1.19",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.70.2",
    "next-test-api-route-handler": "^5.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
