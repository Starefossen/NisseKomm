{
  "name": "nissekomm",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "next build",
    "check": "tsc --noEmit && eslint . && knip && prettier --write --list-different . && jest --silent",
    "cleanup:dev": "tsx scripts/cleanup-test-sessions.ts --dataset development",
    "cron:health": "tsx scripts/test-cron-health.ts",
    "cron:health:prod": "tsx scripts/test-cron-health.ts https://nissekomm.no",
    "dev": "next dev",
    "email:dev": "tsx scripts/send-welcome-emails.ts --dataset development",
    "email:prod": "tsx scripts/send-welcome-emails.ts --dataset production",
    "email:daily-dev": "tsx scripts/send-daily-mission-emails.ts --dataset development",
    "email:daily-prod": "tsx scripts/send-daily-mission-emails.ts --dataset production",
    "format": "prettier --write --list-different .",
    "format:check": "prettier --check .",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "leaderboard:dev": "tsx scripts/leaderboard.ts --dataset development",
    "leaderboard:prod": "tsx scripts/leaderboard.ts --dataset production",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migration:list": "sanity migration list",
    "migration:run:dev": "SANITY_STUDIO_DATASET=development sanity migration run",
    "migration:run:prod": "SANITY_STUDIO_DATASET=production sanity migration run",
    "sanity": "sanity",
    "sanity:dev": "SANITY_STUDIO_DATASET=development sanity",
    "sanity:prod": "SANITY_STUDIO_DATASET=production sanity",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:silent": "jest --silent",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sanity/client": "^7.13.1",
    "@sanity/vision": "^3.99.0",
    "@vercel/analytics": "^1.6.1",
    "date-fns": "^4.1.0",
    "html5-qrcode": "^2.3.8",
    "next": "^16.0.7",
    "pixelarticons": "^1.8.1",
    "qr-code-styling": "^1.9.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.5.2",
    "sanity": "^4.19.0",
    "styled-components": "^6.1.19",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@sanity/migrate": "^4.19.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.71.0",
    "next-test-api-route-handler": "^5.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
